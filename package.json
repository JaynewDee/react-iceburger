{
  "name": "react-iceburger",
  "version": "0.1.11",
  "author": {
    "name": "Joshua Newell Diehl"
  },
  "homepage": "https://github.com/JaynewDee/react-iceburger",
  "license": "MIT",
  "description": "A customizable menu component library",
  "keywords": [
    "react",
    "component",
    "menu",
    "hamburger",
    "drawer"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-iceburger": "^0.1.11"
  },
  "devDependencies": {
    "@types/node": "^20.5.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "exports": {
    ".": {
      "import": [
        "./dist/react-iceburger.es.js",
        "./dist/index.d.ts"
      ],
      "require": [
        "./dist/react-iceburger.umd.js",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "types": "dist/index.d.ts"
}